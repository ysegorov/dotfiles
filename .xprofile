#!/bin/sh
#
# ~/.xprofile
#

xrandr --output LVDS --primary

# if we have private ssh key(s), start ssh-agent and add the key(s)

id1=$HOME/.ssh/id_chtd_rsa
id2=$HOME/.ssh/id_dsa
id3=$HOME/.ssh/id_rsa
if [ -x /usr/bin/ssh-agent ] && [ -f $id1 -o -f $id2 -o -f $id3 ];
then
    eval `ssh-agent -s`
    ssh-add $id1 $id2 $id3 < /dev/null
fi

